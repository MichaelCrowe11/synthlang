{
  "name": "synthlang-api",
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "SYNTH_VERSION": "0.1.0"
  },
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  }
}